									<td style="padding: 0.3rem;"><div style="margin-top: 10px;margin-left: 10px">${contact.phone }</div></td>
									<td style="padding: 0.3rem;"><div style="margin-top: 10px;"><U>${contact.email }</U></div></td>
									<td style="padding: 0.3rem;"><div style="margin-top: 10px;">${contact.dname }</div></td>
									<td style="padding: 0.3rem;"><div style="margin-top: 10px;margin-left: 10px">${contact.call }</div></td>
